generator client {
  provider = "prisma-client-js"
  output   = "../src/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id          Int      @id
  phoneNumber String   @unique
  name        String
  visits      Visit[]
}

model Visit {
  id          Int      @id @default(autoincrement())
  timestamp   DateTime
  description String
  type        String
  patient     Patient  @relation(fields: [patientId], references: [id])
  patientId   Int
}

